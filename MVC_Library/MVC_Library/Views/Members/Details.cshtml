@model MVC_Library.Models.Member
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.MembershipType.Name</li>
    @if(Model.BirthDate != default(DateTime))
    {
        <li>Birthdate: @Model.BirthDate.ToLongDateString()</li>
    }
</ul>

